
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/fe2c0000.mmc/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option channel '36'
	option band '5g'
	option htmode 'VHT80'
	option country 'US'
	option legacy_rates '1'
	option mu_beamformer '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'JPTV_Wireless'
	option encryption 'psk2'
	option key 'password'

